    <mapping resource="hibernate/mapping/Showhideuplaccpic.hbm.xml"/>
    <mapping resource="hibernate/mapping/Userchatlist.hbm.xml"/>
    <mapping resource="hibernate/mapping/Devicedetails.hbm.xml"/>
    <mapping resource="hibernate/mapping/Chatuseroptions.hbm.xml"/>
    <mapping resource="hibernate/mapping/Showhideinfo.hbm.xml"/>
    <mapping resource="hibernate/mapping/Blocklist.hbm.xml"/>
    <mapping resource="hibernate/mapping/Disabledata.hbm.xml"/>
    <mapping resource="hibernate/mapping/Online.hbm.xml"/>
    <mapping resource="hibernate/mapping/Posts.hbm.xml"/>
    <mapping resource="hibernate/mapping/Wooks.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
